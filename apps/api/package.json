  "dependencies": {
    "@adonisjs/auth": "^9.2.3",
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.3.0",
    "@adonisjs/session": "7.5.0",
    "@vinejs/vine": "^2.1.0",
