    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "1.9.3",
    "@types/luxon": "3.4.2",
    "@types/node": "^22.7.5",
    "eslint": "^9.12.0",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^11.2.2",
    "ts-node-maintained": "^10.9.4",
    "typescript": "~5.7.0"
  },
  "hotHook": {
    "boundaries": [
